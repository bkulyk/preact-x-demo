{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    ["@babel/react"],
    ["react-app"],
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "transform-react-jsx",
    "@babel/plugin-proposal-function-bind",
  ]
}
